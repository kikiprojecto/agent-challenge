{
  "version": "0.1",
  "type": "container",
  "meta": {
    "trigger": "cli",
    "system_requirements": {
      "cpu": 2,
      "memory": 4096
    }
  },
  "ops": [
    {
      "type": "container/run",
      "id": "neurocoder-ai",
      "args": {
        "image": "kikiprojecto/neurocoder-ai:v1",
        "gpu": false,
        "entrypoint": ["npm"],
        "cmd": ["start"],
        "env": {
          "OLLAMA_API_URL": "http://localhost:11434/api",
          "MODEL_NAME_AT_ENDPOINT": "qwen3:8b",
          "NODE_ENV": "production",
          "PORT": "3000"
        }
      }
    }
  ]
}
